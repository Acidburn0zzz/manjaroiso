        APPEND vga.cfg misovar=,,,video:nouveau,composite:off

LABEL video:nvidia
        MENU LABEL Nvidia Proprietary (nvidia) with composite
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:nvidia,composite:on

LABEL video:nvidianocomp
        MENU LABEL Nvidia Proprietary (nvidia)
