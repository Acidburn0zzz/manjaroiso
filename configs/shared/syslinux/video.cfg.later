DEFAULT vesamenu.c32
MENU TITLE Select Video Driver

INCLUDE common.cfg

MENUPARAM misovar 3

LABEL video:auto
        MENU LABEL Auto
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:auto,composite:on
ENDTEXT

LABEL video:intel
        MENU LABEL Intel (intel) with composite
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:i915,composite:on

LABEL video:intelnocomp
        MENU LABEL Intel (intel)
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:i915,composite:off

LABEL video:radeon
        MENU LABEL Ati Radeon Open Source (radeon) with composite
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:radeon,composite:on

LABEL video:radeonnocomp
        MENU LABEL Ati Radeon Open Source (radeon)
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:radeon,composite:off

LABEL video:fglrx
        MENU LABEL Ati proprietary (fglrx) with composite
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:fglrx,composite:on

LABEL video:fglrxnocomp
        MENU LABEL Ati proprietary (fglrx)
        KERNEL manjaro.c32
        APPEND vga.cfg misovar=,,,video:fglrx,composite:off
LABEL video:nv
        MENU LABEL Nvidia 2D only (nv)
